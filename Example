setwd("~/Desktop") ### set working directory, for example I saved the Data.csv in my Desktop
data <- read.csv("Data.csv") ### read csv data
X1 <- data$X1
X2 <- data$X2
X <- cbind(X1,X2)
y <- data$test.result
c <- data$observed.time

###########This function will call the packages: "dlm", "stats", "MASS", "coda" 

RegPO_MCSD(X=X,y=y,c=c,alpha=0.9,beta=0.9,m.knots=6)





